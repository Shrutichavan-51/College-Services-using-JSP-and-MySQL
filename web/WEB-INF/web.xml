<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    
 <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>db.SignupServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/SignupServlet</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>db.LoginServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>


<servlet>
        <servlet-name>CollegeSearchServlet</servlet-name>
        <servlet-class>CollegeSearchServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CollegeSearchServlet</servlet-name>
        <url-pattern>/CollegeSearchServlet</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
